{
   "version": 2,
   "public": true,
   "scope": "caenguidanos",
   "builds": [
      {
         "src": "coverage/**",
         "use": "@vercel/static"
      }
   ],
   "routes": [
      {
         "src": "/(.*).(js.map|js|css|txt|html|png|json)",
         "dest": "/coverage/$1.$2",
         "headers": {
            "cache-control": "no-cache"
         }
      },
      {
         "src": "/(.*)",
         "dest": "/coverage/index.html",
         "headers": {
            "Cache-Control": "no-cache"
         }
      }
   ]
}
