      "build": "next build",
      "lint": "eslint .",
      "test": "pnpm msw:exec && NODE_OPTIONS=--experimental-vm-modules jest --no-cache",
      "e2e": "PLAYWRIGHT_MODE='default' playwright test",
      "e2e-ci": "PLAYWRIGHT_MODE='ci-cd' playwright test",
      "e2e-storybook": "PLAYWRIGHT_MODE='storybook' playwright test",
      "format": "prettier --write .",
      "migrate": "ncu --peer --target latest > .migration",
      "migrate:write": "ncu -u --target latest",
      "storybook": "start-storybook -p 6006 -s public",
      "build-storybook": "build-storybook -s public",
      "commitlint": "commitlint",
      "msw:exec": "msw init public",
      "prepare": "husky install"
